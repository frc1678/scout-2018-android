    <style name="InstabugAnnotationColorIconContainer">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingRight">4dp</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:foregroundGravity">center</item>
    </style>
    <style name="InstabugAnnotationColorIconImage">
        <item name="android:layout_width">36dp</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
    <style name="InstabugAnnotationContainer" parent="InstabugBaseContainer">
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="InstabugBaseContainer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">?attr/instabug_background_color</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:orientation">vertical</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="InstabugBaseText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
